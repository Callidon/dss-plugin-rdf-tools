{
  "meta": {
    "label": "Extract RDF files",
    "description": "Extract RDF files into a dataset",
    "icon": "fas fa-file-import"
  },

  "kind": "PYTHON",

  "inputRoles": [
    {
      "name": "input_managed_folders",
      "label": "Input folder(s)",
      "description": "Folder(s) containing the input files",
      "arity": "NARY",
      "required": true,
      "acceptsDataset": false,
      "acceptsManagedFolder": true
    }
  ],

  "selectableFromFolder": "input_managed_folders",

  "outputRoles": [
    {
      "name": "output_dataset",
      "label": "Output dataset",
      "description": "Dataset where the RDF data will be extracted",
      "arity": "UNARY",
      "required": true,
      "acceptsDataset": true
    }
  ],

  "params": [
    {
      "name": "subject_output_column",
      "label": "Subject column name",
      "type": "COLUMN",
      "columnRole": "output_dataset",
      "defaultValue": "subject",
      "mandatory": true
    },
    {
      "name": "predicate_output_column",
      "label": "Predicate column name",
      "type": "COLUMN",
      "columnRole": "output_dataset",
      "defaultValue": "predicate",
      "mandatory": true
    },
    {
      "name": "object_output_column",
      "label": "Object column name",
      "type": "COLUMN",
      "columnRole": "output_dataset",
      "defaultValue": "object",
      "mandatory": true
    }
  ],

  "resourceKeys": []
}
